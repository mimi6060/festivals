{
  "name": "festivals-mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint .",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:build:ios:release": "detox build --configuration ios.sim.release",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:build:android:release": "detox build --configuration android.emu.release",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "e2e:test:ios:release": "detox test --configuration ios.sim.release",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:test:android:release": "detox test --configuration android.emu.release",
    "e2e:test:ios:auth": "detox test --configuration ios.sim.debug e2e/tests/auth.test.ts",
    "e2e:test:ios:wallet": "detox test --configuration ios.sim.debug e2e/tests/wallet.test.ts",
    "e2e:test:ios:payment": "detox test --configuration ios.sim.debug e2e/tests/payment.test.ts",
    "e2e:test:ios:scanner": "detox test --configuration ios.sim.debug e2e/tests/scanner.test.ts",
    "e2e:test:ios:program": "detox test --configuration ios.sim.debug e2e/tests/program.test.ts",
    "e2e:test:ios:offline": "detox test --configuration ios.sim.debug e2e/tests/offline.test.ts",
    "e2e:test:android:auth": "detox test --configuration android.emu.debug e2e/tests/auth.test.ts",
    "e2e:test:android:wallet": "detox test --configuration android.emu.debug e2e/tests/wallet.test.ts",
    "e2e:test:android:payment": "detox test --configuration android.emu.debug e2e/tests/payment.test.ts",
    "e2e:test:android:scanner": "detox test --configuration android.emu.debug e2e/tests/scanner.test.ts",
    "e2e:test:android:program": "detox test --configuration android.emu.debug e2e/tests/program.test.ts",
    "e2e:test:android:offline": "detox test --configuration android.emu.debug e2e/tests/offline.test.ts",
    "e2e:clean": "detox clean-framework-cache && detox build-framework-cache",
    "e2e:recorder": "detox recorder --configuration ios.sim.debug"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/netinfo": "11.1.0",
    "expo": "~50.0.0",
    "expo-brightness": "~11.8.0",
    "expo-camera": "~14.0.0",
    "expo-crypto": "~12.8.0",
    "expo-font": "~11.10.0",
    "expo-haptics": "~12.8.0",
    "expo-linking": "~6.2.0",
    "expo-location": "~16.5.0",
    "expo-router": "~3.4.0",
    "expo-secure-store": "~12.8.0",
    "expo-splash-screen": "~0.26.0",
    "expo-sqlite": "~13.4.0",
    "expo-status-bar": "~1.11.0",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-maps": "1.8.0",
    "react-native-reanimated": "~3.6.0",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@config-plugins/detox": "^7.0.0",
    "@types/react": "~18.2.45",
    "detox": "^20.14.0",
    "eslint": "^8.56.0",
    "eslint-config-expo": "^7.0.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-expo": "~50.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.3.0"
  },
  "private": true
}
